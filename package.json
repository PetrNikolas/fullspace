{
  "name": "marleyjs",
  "version": "0.8.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "nx:affected:apps": "./node_modules/.bin/nx affected:apps",
    "nx:affected:libs": "./node_modules/.bin/nx affected:libs",
    "nx:affected:build": "./node_modules/.bin/nx affected:build",
    "nx:affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "nx:affected:test": "./node_modules/.bin/nx affected:test",
    "nx:affected:lint": "./node_modules/.bin/nx affected:lint",
    "nx:affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "nx:format": "./node_modules/.bin/nx format:write",
    "nx:format:write": "./node_modules/.bin/nx format:write",
    "nx:format:check": "./node_modules/.bin/nx format:check",
    "nx:workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "nx:dep-graph": "./node_modules/.bin/nx dep-graph",
    "nx:help": "./node_modules/.bin/nx help",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "stylelint": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "lint:ci": "npm run stylelint && npm run lint",
    "prettier": "prettier --config '. prettierrc.json' --write 'apps/*.ts' --write 'apps/**/*.ts'",
    "build:dev": "ng build",
    "build:prod": "ng build --prod && npm run version",
    "serve": "ng serve",
    "serve:proxy": "ng serve --proxy-config proxy.conf.json",
    "version": "node version.js",
    "precommit": "lint-staged"
  },
  "private": true,
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,md,scss}": [
      "npm run affected:lint  -- --uncommitted --fix --parallel",
      "npm run format:write -- --uncommitted",
      "git add"
    ]
  },
  "author": "Petr Nikolas",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@nrwl/nx": "6.4.0",
    "bulma": "^0.7.2",
    "core-js": "^2.5.4",
    "express": "4.16.3",
    "git-describe": "^4.0.3",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@compodoc/compodoc": "^1.1.6",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/builders": "6.4.0",
    "@nrwl/schematics": "6.4.0",
    "@types/express": "4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "describe": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-axe": "^3.1.0",
    "jest-image-snapshot": "^2.7.0",
    "jest-preset-angular": "^6.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.0.4",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "stylelint": "^9.8.0",
    "stylelint-config-css-modules": "^1.3.0",
    "testcafe": "^0.23.2",
    "testcafe-live": "^0.1.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
