{
  "name": "marleyjs",
  "version": "0.7.3",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">05.0.3"
  },
  "private": true,
  "repository": {},
  "description": "Modern Angular 6+ Stack.",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint:styles && yarn lint",
    "prettier": "prettier {src,e2e}/*.ts --write && prettier {src,e2e}/**/*.ts --write",
    "e2e": "ng e2e",
    "setup:project": "yarn install && yarn build:ssr && yarn start",
    "build:client:server": "yarn lint:styles && yarn prettier && yarn build:prod && ng run angular.io-example:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "yarn build:client:server && yarn webpack:server",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "serve": "ng serve",
    "start": "node dist/server.js"
  },
  "author": "Petr Nikolas",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/upgrade": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "angular-in-memory-web-api": "^0.6.0",
    "bulma": "^0.7.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.45",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier": "^1.13.7",
    "protractor": "~5.3.0",
    "sass-lint": "^1.12.1",
    "stylelint": "^9.4.0",
    "stylelint-config-css-modules": "^1.3.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "2.7.2",
    "webpack-cli": "^2.0.14"
  }
}
