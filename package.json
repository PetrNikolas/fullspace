{
  "name": "marleyjs",
  "version": "0.8.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server.js",
    "build": "ng build",
    "test": "jest --watch",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint:styles && yarn lint",
    "conventional-changelog": "conventional-changelog",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app 'yarn start' --app-init-delay 5000",
    "test:ci": "jest --runInBand",
    "test:coverage": "rimraf coverage && jest --coverage",
    "analyzer": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "prettier": "prettier --config '. prettierrc.json' --write 'src/*.ts' --write 'src/**/*.ts'",
    "setup:project": "yarn install && yarn build:ssr && yarn start",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"Marley.js documentation\" -w -o --theme \"postmark\"",
    "compodoc:serve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s -n \"Marley.js documentation\" -w -o --theme \"postmark\"",
    "build:client:server": "yarn lint:styles && yarn prettier && yarn build:prod && ng run angular.io-example:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "yarn lint:ci && yarn build:client:server && yarn webpack:server && node version.js",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "serve": "ng serve",
    "serve:proxy": "ng serve --proxy-config proxy.conf.json",
    "precommit": "lint-staged"
  },
  "private": true,
  "lint-staged": {
    "{src,e2e}/**/*.{ts,tsx}": [
      "yarn lint:ci",
      "yarn prettier",
      "git add"
    ]
  },
  "author": "Petr Nikolas",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@nrwl/nx": "6.4.0",
    "core-js": "^2.5.4",
    "express": "4.16.3",
    "git-describe": "^4.0.3",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@compodoc/compodoc": "^1.1.6",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/builders": "6.4.0",
    "@nrwl/schematics": "6.4.0",
    "@types/express": "4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "describe": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-axe": "^3.1.0",
    "jest-image-snapshot": "^2.7.0",
    "jest-preset-angular": "^6.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.0.4",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "stylelint": "^9.8.0",
    "stylelint-config-css-modules": "^1.3.0",
    "testcafe": "^0.23.2",
    "testcafe-live": "^0.1.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
